RED="\e[31m"
GREEN="\e[32m"
ENDCOLOR="\e[0m"

echo -e "${GREEN}Updating proxy...${ENDCOLOR}"
clear
sleep 1
if [ -f "proxy_linux" ]; then
    echo -e "${RED}Deleting old proxy...${ENDCOLOR}"
    rm proxy_linux
    sleep 1
fi
echo -e "${GREEN}Getting new proxy...${ENDCOLOR}"
wget -q https://raw.githubusercontent.com/Yoshiro0805/Yoshiro0805.github.io/andriod/proxyv2
sleep 1
echo -e "${GREEN}Executing proxy...${ENDCOLOR}"
chmod +x YoshiProxy
./YoshiProxy
